enum Role {
  manager
  member
}

type User {
  userId: ID!
  username: String!
  email: String!
  role: Role!
  createdAt: String!
}

type AuthPayload {
  token: String!
  user: User!
}

type Query {
  fetchUsers: [User!]!
}

type Mutation {
  createUser(username: String!, email: String!, password: String!, role: Role!): User!
  login(email: String!, password: String!): AuthPayload!
  logout: Boolean!
}
